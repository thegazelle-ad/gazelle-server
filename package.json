{
  "name": "the-gazelle",
  "version": "1.0.0",
  "engines": {
    "node": "~9.3.0",
    "npm": "~5.6.0"
  },
  "description": "Minimal starter kit for React, Falcor, and Universal rendering with ES6",
  "main": "server.js",
  "scripts": {
    "start": "node ./build/server.js",
    "build": "webpack --config webpack/webpack.dev.config.js",
    "build:watch": "npm run build -- -w",
    "build:production": "webpack --config webpack/webpack.production.config.js --bail",
    "build:staging": "webpack --config webpack/webpack.staging.config.js --bail",
    "build:emulate-ci": "CI=true CIRCLECI=true npm run build:production",
    "test:unit": "jest --config='__tests__/config/jest.unit.config.js' --testLocationInResults",
    "test:e2e": "jest --config='__tests__/config/jest.e2e.config.js' --testLocationInResults --runInBand",
    "lint": "eslint --max-warnings 0 '{src,__tests__,webpack}/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "db:migrate": "knex migrate:latest",
    "//": [
      "The precommit script below is a hack to allow us to do patch committing, and it then has the special",
      "for when we are merging that it will just run lint-staged as otherwise the git stash messes with gits internal",
      "system of keeping track of merges"
    ],
    "precommit": "if ! [ -s .git/MERGE_HEAD ]; then TEMP_STASH_OUTPUT=$(git stash --keep-index); if [ \"$TEMP_STASH_OUTPUT\" != 'No local changes to save' ]; then lint-staged; PRECOMMIT_PASSED=$(echo $?); MUTE=$(git stash pop); exit $PRECOMMIT_PASSED; fi; exit 0; fi; lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --max-warnings 0",
    "*": "npm run test:unit -- --bail --findRelatedTests"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegazelle-ad/gazelle-front-end.git"
  },
  "keywords": [
    "react",
    "es6",
    "falcor",
    "universal"
  ],
  "author": "Lingliang Zhang (lingliangz@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thegazelle-ad/gazelle-front-end/issues"
  },
  "homepage": "https://github.com/thegazelle-ad/gazelle-front-end#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "json5": "^1.0.1",
    "json5-loader": "^1.0.1",
    "lint-staged": "^5.0.0",
    "nightmare": "^2.10.0",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.11.1",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.169.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "child_process": "^1.0.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "falcor": "^2.0.0",
    "falcor-express": "^0.1.4",
    "falcor-http-datasource": "^0.1.3",
    "falcor-router": "^0.8.1",
    "imagemin": "^5.3.1",
    "imagemin-jpeg-recompress": "^5.1.0",
    "knex": "^0.14.4",
    "lodash": "^4.13.1",
    "material-ui": "^0.17.1",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.14",
